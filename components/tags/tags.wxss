/* 客户印象标签 */

.tags-body {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.3);
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  z-index: 9999;
}

.tags-content {
  width: 660rpx;
  height: 538rpx;
  background-color: #fff;
  border-radius: 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.tags-title {
  font-size: 36rpx;
color: #212125;
}