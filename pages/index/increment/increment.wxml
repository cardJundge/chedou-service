<!--增值服务-->
<view class="increment-body">
	<view class="list-top">
		<view class="search-bg">
			<view class="search-box">
				<view class="search">
					<input placeholder-class="ph-class" type='text' placeholder="搜索业务" bindconfirm="search"></input>
					<image src="/images/personnel/icon_search.png"></image>
				</view>
				<image bindtap="addIncrement" class="add" src="/images/personnel/icon_add.png"></image>
			</view>
		</view>
	</view>
	<view class="list-content">
		<view wx:for="{{incrementList}}" class="list-item" catchtap="toIncrementDetails">
        <view class="list-item-left">
          <text>{{item.name}} | {{item.mobile}}</text>
          <text>{{item.case_no}}</text>
        </view>
        <view class='list-item-right status01'>未开通</view>
      </view>
	</view>
</view>